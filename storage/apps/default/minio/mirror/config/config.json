{
  "version": "10",
  "aliases": {
          "minio": {
                  "url": "http://minio.default.svc.cluster.local:9000",
                  "accessKey": "{{ .MINIO_ROOT_USER }}",
                  "secretKey": "{{ .MINIO_ROOT_PASSWORD }}",
                  "api": "s3v4",
                  "path": "auto"
          },
          "cloudflare": {
                  "url": "https://{{ index . 'account-tag' | toString }}.r2.cloudflarestorage.com",
                  "accessKey": "{{ index . 'aws-access-key-id' | toString }}",
                  "secretKey": "{{ index . 'aws-secret-access-key' | toString }}",
                  "api": "S3v4",
                  "path": "auto"
          }
  }
}
