{
  "id": "k8s-5",
  "name": "k8s-5",
  "boot": {
    "kernel": "/assets/k8s-5.kernel-amd64",
    "initrd": ["/assets/k8s-5.initramfs-amd64.xz"],
    "args": [
      "initrd=initramfs-amd64.xz",
      "init_on_alloc=1",
      "slab_nomerge",
      "pti=on",
      "console=tty0",
      "console=ttyS0",
      "printk.devkmsg=on",
      "talos.platform=metal",
      "talos.config=http://192.168.1.128/assets/k8s-5.yaml"
    ]
  }
}
