[start]
command:/usr/local/etc/rc.d/promtail start
parameters:
type:script
message:starting promtail

[stop]
command:/usr/local/etc/rc.d/promtail stop
parameters:
type:script
message:stopping promtail

[restart]
command:/usr/local/etc/rc.d/promtail restart
parameters:
type:script
message:restarting promtail
